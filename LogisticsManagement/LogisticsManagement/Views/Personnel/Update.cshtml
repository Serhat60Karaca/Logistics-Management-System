@model Personnel
<hr />
<fieldset>
    <legend>Update Personnel</legend>
    <form asp-action="Update" method="post">
        <table>
            <tr>
                <td colspan="3"><div asp-validation-summary="All"></div></td>
            </tr>
            <tr>
                <td><label asp-for="Name"></label></td>
                <td><input asp-for="Name" /></td>
                <td><span asp-validation-for="Name"></span></td>
            </tr>
            <tr>
                <td><label asp-for="Contact"></label></td>
                <td><input asp-for="Contact" /></td>
                <td><span asp-validation-for="Contact"></span></td>
            </tr>
            <tr>
                <td colspan="3"><input onclick="window.location='/personnel'" type="submit" value="Update" /></td>
            </tr>
        </table>
    </form>
</fieldset>